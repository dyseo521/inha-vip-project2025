{
  "name": "eecar-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && node scripts/create-function-packages.js",
    "build:layer": "tsc -p tsconfig.layer.json && echo '{\"name\":\"eecar-utils-layer\",\"version\":\"1.0.0\",\"type\":\"module\"}' > dist/layer/nodejs/package.json",
    "build:all": "npm run build && npm run build:layer",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint . --ext ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.515.0",
    "@aws-sdk/lib-dynamodb": "^3.515.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-sns": "^3.515.0",
    "@aws-sdk/client-bedrock-runtime": "^3.515.0",
    "@aws-sdk/client-lambda": "^3.515.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.134",
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
