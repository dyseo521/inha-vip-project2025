{
  "name": "eecar-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && node scripts/create-function-packages.js",
    "build:layer": "tsc -p tsconfig.layer.json && echo '{\"name\":\"eecar-utils-layer\",\"version\":\"1.0.0\",\"type\":\"module\"}' > dist/layer/nodejs/package.json",
    "build:all": "npm run build && npm run build:layer",
    "watch": "tsc --watch",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "lint": "eslint . --ext ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.958.0",
    "@aws-sdk/client-bedrock-runtime": "^3.515.0",
    "@aws-sdk/client-cloudwatch": "^3.958.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.515.0",
    "@aws-sdk/client-lambda": "^3.515.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-s3vectors": "^3.958.0",
    "@aws-sdk/client-sns": "^3.515.0",
    "@aws-sdk/client-ssm": "^3.952.0",
    "@aws-sdk/lib-dynamodb": "^3.515.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/util-stream-node": "^3.370.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/aws-lambda": "^8.10.134",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  }
}
